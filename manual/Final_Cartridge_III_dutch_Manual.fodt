     <text:p text:style-name="P56"><text:soft-page-break/><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc4802_2830447109" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Opmerkingen over de reset<text:tab/>67</text:a></text:p>
     <text:p text:style-name="P56"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc4804_2830447109" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Opmerkingen over Basic<text:tab/>67</text:a></text:p>
     <text:p text:style-name="P56"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc4806_2830447109" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Opmerkingen over muizen<text:tab/>67</text:a></text:p>
     <text:p text:style-name="P56"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc4808_2830447109" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Opmerkingen over de disk-snellader<text:tab/>67</text:a></text:p>
    </text:index-body>
