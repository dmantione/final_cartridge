     <text:p text:style-name="P61"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc6052_3037178849" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Type<text:tab/>27</text:a></text:p>
     <text:p text:style-name="P61"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc6050_3037178849" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Picture<text:tab/>27</text:a></text:p>
     <text:p text:style-name="P61"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc6048_3037178849" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">colors<text:tab/>27</text:a></text:p>
     <text:p text:style-name="P61"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc6046_3037178849" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Printing<text:tab/>27</text:a></text:p>
     <text:p text:style-name="P61"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc6044_3037178849" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Horizontal/ Vertical size<text:tab/>27</text:a></text:p>
     <text:p text:style-name="P61"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc6042_3037178849" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Graphics modes 8/24 pins<text:tab/>27</text:a></text:p>
     <text:p text:style-name="P61"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc6040_3037178849" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Exit<text:tab/>28</text:a></text:p>
     <text:p text:style-name="P61"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc6038_3037178849" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Print<text:tab/>28</text:a></text:p>
     <text:p text:style-name="P61"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc6036_3037178849" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Speciale printerfuncties<text:tab/>28</text:a></text:p>
